{
	"name": "Challenge3",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "Select * , ( \nselect  OrderDetailid, OrderID, Email, ProductID, Quantity, UnitPrice, (\nselect  *  from [dbo].[Item] i where od.ProductId = i.ItemId for json auto) as ProductData \nfrom [dbo].[OrderDetails] od where Od.Orderid = o.Orderid  for json auto) OrderDetailData from Orders o",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"sink": {
									"path": "$"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "OrdersQuery",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "OrdersContainer",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}